<%- include("../partials/header.ejs") %> 
<form action="/cohorts" method="post">
    <div class="form-group">
      <label for="logoUrl">Logo Url</label>
      <input type="url" class="form-control" name="logoUrl" placeholder="e.g. https://mir-s3-cdn-cf.behance.net/project_modules/max_1200/934e7711472343.560f8427810f7.jpg">
    </div>
    <div class="form-group">
      <label for="name">Name</label>
      <input type="text" class="form-control" name="name" placeholder="Flying Funk">
    </div>
    <div class="form-group">
        <label for="members">Members</label>
        <textarea class="form-control" name="members" placeholder="John Snow, Daenerys, Melisandre, Arya Stark" cols="60" rows="5"></textarea> 
    </div>
    <button type="submit" class="btn btn-primary float-right">Submit</button>
</form>

<%- include("../partials/footer.ejs") %> 